{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "./src",
    "paths": {
      "@store/*": ["store/*"],
      "@components/*": ["components/*"],
      "@assets/*": ["assets/*"],
      "@variables/*": ["variables/*"],
      "@hooks/*": ["hooks/*"],
    },
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
  },
}
