{
  "compilerOptions": {
    "outDir": "./dist",
    "jsx": "react",
    "baseUrl": "./src",
    "paths": {
      "@store/*": ["store/*"],
      "@components/*": ["components/*"],
      "@assets/*": ["assets/*"],
      "@variables/*": ["variables/*"],
      "@hooks/*": ["hooks/*"]
    },
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "skipLibCheck": true
  }
}
